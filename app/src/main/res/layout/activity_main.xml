<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#5D026C"
	tools:context=".MainActivity">

	<ImageView
		android:id="@+id/imagePlaying"
		android:layout_width="53dp"
		android:layout_height="57dp"
		android:layout_marginTop="554dp"
		android:layout_marginEnd="179dp"
		android:layout_marginRight="179dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@android:drawable/ic_media_play" />

	<ImageView
		android:id="@+id/imagePrevious"
		android:layout_width="62dp"
		android:layout_height="59dp"
		android:layout_marginTop="552dp"
		android:layout_marginEnd="303dp"
		android:layout_marginRight="303dp"
		android:layout_marginBottom="120dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@android:drawable/ic_media_rew" />

	<ImageView
		android:id="@+id/imageNext"
		android:layout_width="56dp"
		android:layout_height="54dp"
		android:layout_marginStart="304dp"
		android:layout_marginLeft="304dp"
		android:layout_marginTop="554dp"
		android:layout_marginBottom="123dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@android:drawable/ic_media_ff" />

	<ImageView
		android:id="@+id/volume"
		android:layout_width="55dp"
		android:layout_height="55dp"
		android:layout_marginStart="304dp"
		android:layout_marginLeft="304dp"
		android:layout_marginTop="635dp"
		android:layout_marginBottom="41dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@android:drawable/ic_lock_silent_mode_off" />

	<ImageView
		android:id="@+id/zing"
		android:layout_width="230dp"
		android:layout_height="248dp"
		app:layout_constraintBottom_toTopOf="@+id/progressBar"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.5"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@mipmap/ic_zing_circle_background" />

	<ImageView
		android:id="@+id/share"
		android:layout_width="54dp"
		android:layout_height="57dp"
		android:layout_marginStart="178dp"
		android:layout_marginLeft="178dp"
		android:layout_marginTop="634dp"
		android:layout_marginBottom="40dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@android:drawable/ic_menu_share" />

	<ProgressBar
		android:id="@+id/progressBar"
		style="?android:attr/progressBarStyleHorizontal"
		android:layout_width="359dp"
		android:layout_height="13dp"
		android:layout_marginTop="512dp"
		android:layout_marginEnd="23dp"
		android:layout_marginRight="23dp"
		android:background="#D5C1C1"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/currentSong"
		android:layout_width="213dp"
		android:layout_height="48dp"
		android:layout_marginStart="99dp"
		android:layout_marginLeft="99dp"
		android:layout_marginTop="407dp"
		android:text="currentSong"
		android:gravity="center"
		android:textColor="#FFFFFF"
		android:textSize="24sp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
